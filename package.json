{
  "name": "held-karp",
  "version": "2.0.0",
  "description": "Highly optimized exact solutions to the traveling salesman problem using the Heldâ€“Karp algorithm",
  "type": "module",
  "exports": {
    ".": "./src/js-impl/index.js",
    "./wasm": "./src/wasm-impl/index.js"
  },
  "keywords": [
    "traveling salesman",
    "travelling salesman",
    "problem",
    "TSP",
    "Hamiltonian",
